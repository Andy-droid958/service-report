{
  "name": "service-report-backend",
  "version": "1.0.0",
  "description": "Backend API for Service Report Management System",
  "main": "server.js",
  "scripts": {
    "start": "node cluster.js",
    "dev": "nodemon --delay 2 cluster.js",
    "cluster": "node cluster.js",
    "single": "node server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs service-report",
    "pm2:monit": "pm2 monit"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "msnodesqlv8": "^5.1.1",
    "mssql": "^10.0.4",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "puppeteer": "^24.25.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "pm2": "^5.3.0"
  },
  "keywords": [
    "express",
    "service-report",
    "api",
    "mssql"
  ],
